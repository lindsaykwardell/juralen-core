{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "jsRules": {},
  "rules": {
    "quotemark": [true, "single"],
    "trailing-comma": false,
    "arrow-parens": [false],
    "no-shadowed-variable": [false],
    "object-literal-key-quotes": [false],
    "curly": [false],
    "semicolon": [false]
  },
  "rulesDirectory": [],
  "linterOptions": {
    "exclude": ["node_modules/*"]
  }
}
